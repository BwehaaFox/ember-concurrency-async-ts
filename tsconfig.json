{
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "noEmit": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "ember-concurrency-ts": ["."],
      "ember-concurrency-ts/*": ["./*"]
    },
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true
  }
}
